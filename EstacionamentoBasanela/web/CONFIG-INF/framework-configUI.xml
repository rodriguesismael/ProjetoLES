        <logic name="AlterarModelo" type="negocio.modelo.AlterarModelo"></logic>
        <!-- Cliente -->
        <logic name="BuscarCliente" type="negocio.cliente.BuscarCliente"></logic>
        <logic name="CadastrarCliente" type="negocio.cliente.CadastrarCliente"></logic>
        <!-- ClienteXVeiculo -->
        <logic name="CadastrarClienteXVeiculo" type="negocio.clienteXVeiculo.CadastrarClienteXVeiculo"></logic>
