<!DOCTYPE config SYSTEM "framework-config.dtd">
<config>
    <logics>
        <!-- Login -->
        <logic name="EfetuarLogin" type="negocio.outro.EfetuarLogin"></logic>
        <!-- Veiculo -->
        <logic name="BuscarVeiculo" type="negocio.veiculo.BuscarVeiculo"></logic>
        <logic name="CadastrarVeiculo" type="negocio.veiculo.CadastrarVeiculo"></logic>
